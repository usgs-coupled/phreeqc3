#PRINT; -reset false
# SOLUTION_SPECIES
# Ca+2 = Ca+2
	# -gamma	5.0	0.1650
	# -dw	 0.793e-9
	# -Vm   0.8 -7.25  0.51  -2.48   1.84  3.64   0.073   17.8 # supcrt modified
	# -Vm   0.8 -7.25  0.51  -2.48   1.84 # 3.64   0.073   17.8 # supcrt modified
# Na+ = Na+
	# -gamma	4.0	0.075
	# -dw	 1.33e-9
	# -Vm   1.53 -2.285 4.57 -2.726   0.169   2.6 -0.28 44 # supcrt modified
	# -Vm   1.53 -2.285 4.57 -2.726   0.169  # 2.6 -0.28 44 # supcrt modified
# Cl- = Cl-
	# -gamma	3.5	0.015
	# -dw	 2.03e-9
	# -Vm  4.03 5.9 5.27 -3.09   1.734   0 -0.3 18 # supcrt modified
	# -Vm  4.03 5.9 5.27 -3.09   1.734 #  0 -0.3 18 # supcrt modified

SOLUTION 1; pH 7 charge
END

