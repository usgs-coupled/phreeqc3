DATABASE C:\Programs\phreeqc3-trunk\database\phreeqc.dat
SOLUTION 10 Test solution
    temp      25
    pH        7
    pe        4
    redox     pe
    units     mmol/kgw
    density   1
    -water    1 # kg
    Na		1000
    Cl		1000
    C		1
    P		1
    S(6)        1
    Ca		1  charge
END
SURFACE 1
    -sites DENSITY
    Hfo_wOH    3         600       1
    Hfo_sOH    0.06
    -donnan 1e-008
END
EQUILIBRIUM_PHASES  1
   Calcite  0 .02 
END
SOLID_SOLUTION 1
 FeZnCO3
 -comp1 Siderite 0.1
 -comp2 Smithsonite 0.1
 -Gugg_non 4
END
KINETICS
Organic_C
    -formula  C  1
    -m        .1
    -m0       .11
    -tol      1e-008
    -steps       8640 86400000
END
GAS_PHASE 1
    -fixed_volume
    -pressure 1
    -volume 1
    -temperature 25
    CO2(g)    0.003
    N2(g)     0.79
    O2(g)     0.2
END
EXCHANGE 1
    X 0.001
    -equil 10
END
REACTION
	O2   1
	0.0001 mol
END
USE solution 10
USE surface 1
USE equilibrium_phases 1
USE kinetics 1
USE solid_solution 1
USE gas_phase 1
USE reaction 1
USE exchange 1
USER_PRINT
-start
10 PRINT "INCREMENTAL_REACTIONS false"
20 PRINT "  Total time     Kin_time    Kin_delta Avg_Rate(mol/s)"
30 PRINT TOTAL_TIME, KIN_TIME, KIN_DELTA("Organic_C"),KIN_DELTA("Organic_C")/KIN_TIME 
-end
END
INCREMENTAL_REACTIONS true
USE solution 10
USE surface 1
USE equilibrium_phases 1
USE kinetics 1
USE solid_solution 1
USE gas_phase 1
USE reaction 1
USE exchange 1
USER_PRINT
-start
10 PRINT "INCREMENTAL_REACTIONS true"
20 PRINT "  Total time     Kin_time    Kin_delta Avg_Rate(mol/s)"
30 PRINT TOTAL_TIME, KIN_TIME, KIN_DELTA("Organic_C"),KIN_DELTA("Organic_C")/KIN_TIME 
-end
END