DATABASE ../database/phreeqc.dat
#   Make oxygenated carbonate-ground-water for infilling solution
SOLUTION 0
        temp    25.0
        pH      7.0 
        Na      1
	Cl      1
	O(0)	1 O2(g)  -0.7
	C(4)	1 CO2(g) -3.5
END
EQUILIBRIUM_PHASES 0
        Calcite	   0.0
        Dolomite   0.0
	CO2(g)     -3.5
	O2(g)	   -0.7
END
USE solution 0
USE equilibrium_phases 0
SAVE solution 0
END
#   Make carbonate-ground-water for initial solution in column
SOLUTION 1
        temp    25.0
        pH      7.0 
        Na      1
	Cl      1
END
EQUILIBRIUM_PHASES 1
        Calcite	   0.0	1.0
        Dolomite   0.0	1.0
	Goethite   0.0	0.0	   
	Gypsum	   0.0  0.0
	Pyrite	   0.0  0.01
	CO2(g)     -2
END
USE solution 1
USE equilibrium_phases 1
SAVE solution 1-10
END
#   No additional CO2 gas in column
#   goethite and gypsum not present initially
#   1 mol/kgw calcite, dolomite
#   .01 mol/kgw pyrite
EQUILIBRIUM_PHASES 1-10
        Calcite	   0.0	1.0
        Dolomite   0.0	1.0
	Goethite   0.0	0.0	   
	Gypsum	   0.0  0.0
	Pyrite	   0.0  0.01
END
#   Transport one pore volume of oxygenated water into 10-cell column
#   Column length 10 m and velocity 1 m/hr
#   All reaction probably occurs in first cell
TRANSPORT
        -cells  10
        -shifts 10
	-flow_direction forward
        -bcon   flux flux
        -length  1.0
	-disp    0.1
        -timest 3600
	-print_frequency 5
SELECTED_OUTPUT
        -file eary.sel
        -totals Ca C(4) S(6) Fe
	-equi   calcite dolomite pyrite goethite gypsum
END
