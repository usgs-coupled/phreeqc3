DATABASE ../database/pitzer.dat
RATES
Halite
	-start
20      rem   1 hour half life
40	moles = log(0.5) / 3600 * m * time
50      if (moles > m) then moles = m
60      rem print -moles
200	save -moles
	-end
SOLUTION 1
	pH	7.0
	pe	8.0
	temp	20.0
	Ca	4.5
	Cl	9.0	charge
EXCHANGE 1
	NaX	Halite 		kine	.1
KINETICS 1
	Halite
		-formula NaCl 1.0 
		-m	1
	-step	3600
END

